<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="main.javafx.controllers.tabs.variables.VariablesController">
    <TableView fx:id="tableView"
               GridPane.columnIndex="0"
               GridPane.rowIndex="0"
               GridPane.vgrow="ALWAYS"
               GridPane.hgrow="ALWAYS">
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
        <columns>
            <TableColumn fx:id="name" prefWidth="125.0" text="variable name"/>
            <TableColumn fx:id="environments" prefWidth="115.0" text="environments"/>
            <TableColumn fx:id="valueAliases" prefWidth="115.0" text="value aliases"/>
        </columns>
    </TableView>
</GridPane>

